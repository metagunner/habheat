    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug habheath",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "hideSystemGoroutines": true,
            "console": "integratedTerminal",
          },
        {
            "name": "Attach to a running Habheath",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1"
          },
    ]
}